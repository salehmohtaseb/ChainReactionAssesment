{
  "name": "Chain-Reaction-assessment",
  "version": "1.0.0",
  "description": "This repo is to solve Chain-Reaction assesment",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "db:create": "npx sequelize-cli db:create --encoding utf8",
    "db:drop": "npx sequelize-cli db:drop",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo",
    "db:seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "start:dev": "nodemon ./src/index.ts"
  },
  "keywords": [
    "nodejs",
    "express",
    "rest",
    "api",
    "sequelize",
    "mysql"
  ],
  "author": "Saleh Almohtaseb <saleh.a.almohtaseb@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^2.0.12",
    "sequelize-cli": "^6.2.0"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.13.0",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.3.5",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "express-session": "^1.17.3",
    "joi": "^17.6.0",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "redis": "^4.2.0",
    "sequelize": "^6.21.0",
    "swagger-jsdoc": "^6.2.4",
    "swagger-ui-express": "^4.5.0",
    "uuid": "^8.3.2"
  }
}
